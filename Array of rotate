class Main {
    public static void main(String[] args) {
       int[] arr={2,7,8,11,10,5};
       int n=3;
      
       for(int i=0;i<n;i++){
           int lastelement=arr[arr.length-1];
           for(int j=arr.length-1;j>0;j--){
               arr[j]=arr[j-1];
           }
           arr[0]=lastelement;
       }
           
       System.out.print("Array After Rotate:");
       for(int i=0;i<arr.length;i++){
           System.out.print(arr[i]+" ");
       }
    }
}
