class Main {
    public static void main(String[] args) {
     int[][]a={{1,2,3,4},
               {4,5,8,9},
               {4,6,7,8},
               {4,8,9,1}};
     int sum=0;           
               
     for(int i=0;i<a.length;i++){
         for(int j=0;j<a.length;j++){
             
             sum=sum+a[i][j];
            
         }
     }  
      System.out.println(sum);
    }
}
       
