import java.util.Scanner;
class Main {
    public static void main(String[] args) {
      Scanner scan=new Scanner(System.in);
      System.out.print("Enter a String: ");
       String a=scan.nextLine();
       String b="";
       for(int i=0;i<a.length();i++){
           char d=a.charAt(i);
        if(!Character.isWhitespace(d)){
            b=b+d;
        }   
       }
      System.out.print(b);    
    }
}
